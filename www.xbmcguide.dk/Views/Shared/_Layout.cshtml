<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.17.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Base.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Content/Less/Stylesheet.less.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/jquery-ui-1.8.17.custom.css")" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="page cf">
        <header>
            <div id="title">
                <div id="TextLogo"><span id="TextLogo1stPart">XBMC</span> <span id="TextLogo2stPart">Guide</span></div>
            </div>
            <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            <div id="MenuWrapper">
                @Html.Action("Menu", "Menu")
            </div>
        </header>
        <div id="MenuContentSpacer"></div>
        <div id="Content">
            <div id="ContentLeft" class="cf">
                @RenderBody()
            </div>
            <div id="ContentRight">
                <input type="text" name="Search" id="Search" placeholder="Søg på XBMC guide" />
            </div>
        </div>
        <div class="cf">
        </div>
    </div>
    <footer>
            This is our footer
    </footer>
</body>
</html>
